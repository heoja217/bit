<script type="text/javascript" src='../js/jquery-1.11.1.js'></script>
<script type="text/javascript">

var source = '<p>Hello, {{name}}</p>';
var template = Handlebars.compile(source);

var html = template({name: '홍길동'});

$('#d1').html(html);






</script>


